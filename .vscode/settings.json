{
	// Typescript use deno
	"prettier.enable": false,
	"deno.enable": true,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "denoland.vscode-deno",
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	}
}